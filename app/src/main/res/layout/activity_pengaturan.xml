<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_welcome"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PengaturanActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">
        <TextView
            android:text="Keamanan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="100dp"
            android:id="@+id/textatur1"
            android:textSize="20sp" />
        <TextView
            android:text="Notifikasi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:id="@+id/textatur2"
            android:layout_below="@+id/textatur1"
            android:textSize="20sp" />
        <TextView
            android:text="Pengawasan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:id="@+id/textatur3"
            android:layout_below="@+id/textatur2"
            android:textSize="20sp" />
        <TextView
            android:text="Bantuan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:id="@+id/textatur4"
            android:layout_below="@+id/textatur3"
            android:textSize="20sp" />
        <TextView
            android:text="Tema"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:id="@+id/textatur5"
            android:layout_below="@+id/textatur4"
            android:textSize="20sp" />
    </RelativeLayout>

</RelativeLayout>